<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.purple.demo.mapper.FeedMapper">
    <select id="selFeedList" resultType="FeedEntity">
        SELECT * FROM feed
        ORDER BY feed_pk DESC
    </select>

    <select id="selFeed" resultType="FeedEntity">
        SELECT * FROM feed
        WHERE feed_pk = #{feed_pk}
    </select>

    <select id="selMediaList" resultType="MediaEntity">
        SELECT * FROM media
        WHERE media_pk = #{media_pk}
    </select>

    <select id="selHashtagList" resultType="HashtagEntity">
        SELECT * FROM hashtag
        WHERE hashtag_pk = #{hashtag_pk}
    </select>

    <select id="selUserList" resultType="UserEntity">
        SELECT * FROM user
        WHERE user_pk = #{user_pk}
    </select>
    
</mapper>